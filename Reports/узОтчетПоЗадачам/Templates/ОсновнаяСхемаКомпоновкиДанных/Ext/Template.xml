		<query>ВЫБРАТЬ
	узЗадачи.Ссылка КАК Задача,
	узЗадачи.Код КАК НомерЗадачи,
	узЗадачи.Автор КАК Автор,
	узЗадачи.Исполнитель КАК Исполнитель,
	узЗадачи.Статус.ВидСтатуса КАК ВидСтатуса,
	узЗадачи.Статус КАК Статус,
	узЗадачи.Конфигурация КАК Конфигурация,
	узЗадачи.Спринт КАК Спринт,
	узЗадачи.Важность КАК Важность,
	узЗадачи.ОсновнаяЗадача КАК ОсновнаяЗадача,
	узЗадачи.РеквизитДопУпорядочивания КАК РеквизитДопУпорядочивания,
	узЗадачи.ДатаСоздания КАК ДатаСоздания,
	узЗадачи.ДатаПередачиИсполнителю КАК ДатаПередачиИсполнителю,
	узЗадачи.СрокИсполнения КАК СрокИсполнения,
	узЗадачи.НомерВнешнейЗаявки КАК НомерВнешнейЗаявки,
	1 КАК КоличествоЗадач,
	узЗадачи.ЧасыПлан КАК ЧасыПлан,
	СУММА(ВЫБОР
			КОГДА узЗадачиФактическиеЧасы.ДатаНачала &gt;= &amp;НачалоПериода
					И узЗадачиФактическиеЧасы.ДатаНачала &lt;= &amp;КонецПериода
				ТОГДА ВЫБОР
						КОГДА узЗадачиФактическиеЧасы.Исполнитель = &amp;ИсполнительЧасыФакт
							ТОГДА узЗадачиФактическиеЧасы.ЧасыФакт
						ИНАЧЕ 0
					КОНЕЦ
			ИНАЧЕ 0
		КОНЕЦ) КАК ЧасыФакт,
	узЗадачи.Очередь КАК Очередь,
	узЗадачи.аэПодразделение КАК Подразделение,
	узЗадачи.аэИнициатор КАК Инициатор,
	узЗадачи.Ссылка КАК Ссылка,
	узЗадачи.ВидОперации КАК ВидОперации,
	узЗадачи.Источникобращения КАК Источникобращения,
	узЗадачи.Проект КАК Проект,
	ПОДСТРОКА(узЗадачи.Комментарий, 1, 1000) КАК Комментарий
ПОМЕСТИТЬ вт
ИЗ
	Справочник.узЗадачи КАК узЗадачи
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.узЗадачи.ФактическиеЧасы КАК узЗадачиФактическиеЧасы
		ПО (узЗадачиФактическиеЧасы.Ссылка = узЗадачи.Ссылка)
ГДЕ
	узЗадачи.ПоказыватьВОтчетахИКанбанДоске

СГРУППИРОВАТЬ ПО
	узЗадачи.Ссылка,
	узЗадачи.Код,
	узЗадачи.Автор,
	узЗадачи.Исполнитель,
	узЗадачи.Статус.ВидСтатуса,
	узЗадачи.Статус,
	узЗадачи.Конфигурация,
	узЗадачи.Спринт,
	узЗадачи.Важность,
	узЗадачи.ОсновнаяЗадача,
	узЗадачи.РеквизитДопУпорядочивания,
	узЗадачи.ДатаСоздания,
	узЗадачи.ДатаПередачиИсполнителю,
	узЗадачи.СрокИсполнения,
	узЗадачи.НомерВнешнейЗаявки,
	узЗадачи.ЧасыПлан,
	узЗадачи.Очередь,
	узЗадачи.аэПодразделение,
	узЗадачи.аэИнициатор,
	узЗадачи.ВидОперации,
	узЗадачи.Источникобращения,
	узЗадачи.Проект,
	ПОДСТРОКА(узЗадачи.Комментарий, 1, 1000),
	узЗадачи.Ссылка
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	вт.Задача,
	вт.НомерЗадачи,
	вт.Автор,
	вт.Исполнитель,
	вт.ВидСтатуса,
	вт.Статус,
	вт.Конфигурация,
	вт.Спринт,
	вт.Важность,
	вт.ОсновнаяЗадача,
	вт.РеквизитДопУпорядочивания,
	вт.ДатаСоздания,
	вт.ДатаПередачиИсполнителю,
	вт.СрокИсполнения,
	вт.НомерВнешнейЗаявки,
	вт.КоличествоЗадач,
	вт.ЧасыПлан,
	вт.ЧасыФакт,
	вт.Очередь,
	вт.Подразделение,
	вт.Инициатор,
	вт.Ссылка,
	вт.ВидОперации,
	вт.Источникобращения,
	вт.Проект,
	вт.Комментарий,
	РАЗНОСТЬДАТ(МАКСИМУМ(узЗадачиИсторияСтатусов.ДатаНачала), &amp;ТекущаяДата, ДЕНЬ) КАК СрокПроверки
ИЗ
	вт КАК вт
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.узЗадачи.ИсторияСтатусов КАК узЗадачиИсторияСтатусов
		ПО вт.Задача = узЗадачиИсторияСтатусов.Ссылка

СГРУППИРОВАТЬ ПО
	вт.Задача,
	вт.НомерЗадачи,
	вт.Автор,
	вт.Исполнитель,
	вт.ВидСтатуса,
	вт.Статус,
	вт.Конфигурация,
	вт.Спринт,
	вт.Важность,
	вт.ОсновнаяЗадача,
	вт.РеквизитДопУпорядочивания,
	вт.ДатаСоздания,
	вт.ДатаПередачиИсполнителю,
	вт.СрокИсполнения,
	вт.НомерВнешнейЗаявки,
	вт.КоличествоЗадач,
	вт.ЧасыПлан,
	вт.ЧасыФакт,
	вт.Очередь,
	вт.Подразделение,
	вт.Инициатор,
	вт.Ссылка,
	вт.ВидОперации,
	вт.Источникобращения,
	вт.Проект,
	вт.Комментарий</query>
